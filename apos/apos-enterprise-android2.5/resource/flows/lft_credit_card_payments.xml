<?xml version="1.0" encoding="UTF-8"?>
<flow-diagram
    begin-node="start"
    version="1.0" >

    <flow-node
        node-name="start"
        ref-activity="me.andpay.apos.lft.activity.CreditCardPaymentsActivity" >

        <node-complete
            identity="credit_card_confirm"
            next-node="credit_card_confirm" />
    </flow-node>

    <flow-node
        node-name="credit_card_confirm"
        ref-activity="me.andpay.apos.lft.activity.CreditCardConfirmActivity" >

        <!-- 信用卡还款 -->

        <node-complete
            identity="lft_card_pay_txn"
            sub-finish-cpl="finished"
            sub-flow="com_txn_flow" />

        <node-complete-finished
            finish-flow="true"
            flush-context="true"
            identity="finish"
            ref-activity="me.andpay.apos.common.activity.HomePageActivity" />
    </flow-node>

</flow-diagram>